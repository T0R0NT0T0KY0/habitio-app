{
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./",
    "paths": {
      "@Constants/*": [
        "src/common/constants/*"
      ],
      "@Components/*": [
        "src/components/*"
      ],
      "@Request": [
        "src/common/requests/Axios"
      ],
	  "@Assets/*": [
		"src/assets/*",
	  ],
	  "@Services/*": [
		"src/services/*"
	  ],
    }
  },
  "extends": "expo/tsconfig.base"
}
